{
    "prod": {
        "sparkTask": {
            "file": "$LAKE_MODULES_DIR/main.py",
            "executorMemory": "8g",
            "executorCores": 5,
            "numExecutors": 5,
            "driverMemory": "10g",
            "driverCores": 2,
            "queue": "TSF",
            "archives": [
                "$LAKE_PKG_TARGET#venv"
            ],
            "args": [
                "--output_directory",
                "${LAKE_HOME}/predictions/production/${CI_PIPELINE_ID}",
                "--configuration",
                "${PREDICTION_CONFIG_FILE}"
            ]
        }
    },
    "small": {
        "sparkTask": {
            "file": "$LAKE_MODULES_DIR/main.py",
            "executorMemory": "3g",
            "executorCores": 2,
            "numExecutors": 3,
            "driverMemory": "2g",
            "driverCores": 1,
            "queue": "TSF",
            "archives": [
                "$LAKE_PKG_TARGET#venv"
            ],
            "args": [
                "--sample_ratio",
                "0.0001",
                "--output_directory",
                "${LAKE_HOME}/predictions/test/${CI_PIPELINE_ID}",
                "--configuration",
                "${PREDICTION_CONFIG_FILE}"
            ]
        }
    }
}
